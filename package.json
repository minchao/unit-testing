{
  "name": "unit-testing",
  "version": "0.1.0",
  "description": "Unit testing",
  "license": "MIT",
  "scripts": {
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "lint-fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "jest --detectOpenHandles --forceExit --coverage --verbose",
    "test-first-unit-test": "jest --detectOpenHandles --forceExit --coverage --verbose -t 'Calculator'",
    "test-coverage": "jest --detectOpenHandles --forceExit --coverage --verbose -t 'Coverage'"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.0",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "sinon": "^9.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "dayjs": "^1.8.29"
  }
}
